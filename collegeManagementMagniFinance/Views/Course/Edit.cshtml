@model collegeManagementMagniFinance.Models.CourseModel

@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>



    
    <div class="form-horizontal" ng-app="Courseapp" ng-controller="CourseController"  data-ng-init="loadCourse(@Convert.ToInt32(Request.QueryString["Id"]))">
        <h4>CourseModel</h4>
        <hr />

        <div class="form-group">
            <label class="control-label col-md-2">Name</label>
            <div class="col-md-10">
                <input class="form-control" ng-model="register.Name" />
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" ng-click="updatedata(register)" class="btn btn-default" />
            </div>
        </div>
    </div>

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
